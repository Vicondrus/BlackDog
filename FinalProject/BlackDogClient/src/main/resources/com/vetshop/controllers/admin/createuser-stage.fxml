<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="461.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="com.vetshop.controllers.admin.CreateUserController">
    <children>
        <Label layoutX="158.0" layoutY="71.0" text="Username"/>
        <Label layoutX="160.0" layoutY="149.0" text="Password"/>
        <Label layoutX="156.0" layoutY="302.0" text="Full Name"/>
        <TextField fx:id="username" layoutX="323.0" layoutY="67.0"/>
        <TextField fx:id="fullname" layoutX="323.0" layoutY="298.0"/>
        <PasswordField fx:id="password" layoutX="323.0" layoutY="145.0"/>
        <Label layoutX="137.0" layoutY="231.0" text="Confirm Password"/>
        <PasswordField fx:id="confirm" layoutX="323.0" layoutY="227.0"/>
        <Button layoutX="275.0" layoutY="354.0" mnemonicParsing="false" onAction="#create" text="Create"/>
        <Button layoutX="280.0" layoutY="405.0" mnemonicParsing="false" text="Back" onAction="#back"/>
    </children>
</AnchorPane>
