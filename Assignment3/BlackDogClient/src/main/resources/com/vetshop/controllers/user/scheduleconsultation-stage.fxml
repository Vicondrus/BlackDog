<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="457.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="com.vetshop.controllers.user.ScheduleConsultationController">
    <children>
        <Label layoutX="163.0" layoutY="70.0" text="Doctor"/>
        <Label layoutX="164.0" layoutY="136.0" text="Patient"/>
